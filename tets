Private Function testMethod(ByVal testID As String, ByVal testList As String) As Boolean
  On Error GoTo ErrorHandler

  Dim oRegEx As New VBScript_RegExp_55.RegExp
  Dim common As Object
  
  oRegEx.IgnoreCase = True
  oRegEx.Pattern = "^(.+,)*" & testID & "(,.+)*$"
  
  IsAFederalGovernmentId = oRegEx.Test(testList)
  
  Exit Function
  
ErrorHandler:
  
  If common Is Nothing Then
    Set common = CreateObject("services.common")
    oWebCommon.LogError Err.Number, Err.Source & vbCrLf & "in " & g_AppName & "." & g_ModuleName, Err.Description, g_ModuleName, True
    Set common = Nothing
  End If

End Function
